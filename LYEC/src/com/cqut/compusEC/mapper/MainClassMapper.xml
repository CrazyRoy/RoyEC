<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cqut.compusEC.dao.MainClassDAO">

	<resultMap type="java.util.HashMap" id="MainClassResult">
		<id property="main_id" column="main_id"/>
		<result property="className" column="className"/>
		<result property="cssName" column="cssName"/>
		<result property="desc" column="desc"/>
	</resultMap> 
	
	<select id="queryAllMainClass" resultMap="MainClassResult">
		select * from tb_product_mainclass
	</select>
	
	<select id="queryMainClassNameById" parameterType="Integer" resultMap="MainClassResult">
		select tb_product_mainclass.className from tb_product_mainclass where main_id = #{main_id}
	</select>
	
</mapper>